{
  "name": "Github Pull Request Template",
  "description": "Blueprint for Pull Request workflows.",
  "version": "0.1.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "background": {
    "scripts": [
      "script/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "clipboardRead",
    "*://*.github.com/*"
  ],
  "browser_action": {
      "default_title": "Bring it on!",
      "default_icon": {
        "19": "icon19.png",
        "38": "icon38.png"
      }
  },
  "manifest_version": 2
}
